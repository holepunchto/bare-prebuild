{
  "name": "bare-prebuild",
  "version": "1.0.2",
  "description": "Tool for recursively prebuilding installed native addons from source",
  "exports": {
    ".": "./index.js",
    "./package": "./package.json"
  },
  "bin": {
    "bare-prebuild": "bin.js"
  },
  "files": [
    "index.js",
    "bin.js",
    "lib"
  ],
  "imports": {
    "child_process": {
      "bare": "bare-subprocess",
      "default": "child_process"
    },
    "fs": {
      "bare": "bare-fs",
      "default": "fs"
    },
    "os": {
      "bare": "bare-os",
      "default": "os"
    },
    "path": {
      "bare": "bare-path",
      "default": "path"
    },
    "process": {
      "bare": "bare-process",
      "default": "process"
    }
  },
  "scripts": {
    "test": "prettier . --check"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/holepunchto/bare-prebuild.git"
  },
  "author": "Holepunch",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/holepunchto/bare-prebuild/issues"
  },
  "homepage": "https://github.com/holepunchto/bare-prebuild#readme",
  "dependencies": {
    "bare-fs": "^4.0.1",
    "bare-make": "^1.4.0",
    "bare-os": "^3.2.1",
    "bare-path": "^3.0.0",
    "bare-process": "^4.0.0",
    "bare-subprocess": "^5.0.2",
    "paparam": "^1.5.0"
  },
  "devDependencies": {
    "brittle": "^3.2.1",
    "prettier": "^3.3.3",
    "prettier-config-standard": "^7.0.0"
  }
}
